<%= form_for @current_user, url: user_path do |f| %>
  <h6 class="heading-small text-muted mb-4" style="padding: 20px; font-size: 20px;">User information</h6>
  <div class="pl-lg-4">
    <div class="row">
      <div class="col-lg-4">
        <div class="form-group focused" style="width: auto; padding: 10px;">
          <label class="form-control-label" for="input-username">Username</label>
          <input type="text" id="input-username" class="form-control form-control-alternative" placeholder="Username" value="lucky.jesse" readonly="readonly">
        </div>
      </div>
      <div class="col-lg-4">
        <div class="form-group focused" style="width: auto; padding: 10px;">
          <label class="form-control-label" for="input-email">Email address</label>
          <%= f.email_field :email, autofocus: true, placeholder: "Name", class: "form-control" %>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-4">
        <div class="form-group focused" style="width: auto; padding: 10px;">
          <label class="form-control-label" for="input-first-name">Name</label>
          <%= f.text_field :name, autofocus: true, placeholder: "Name", class: "form-control" %>
        </div>
      </div>
      <div class="col-lg-4">
        <div class="form-group focused" style="width: auto; padding: 10px;">
          <label class="form-control-label" for="input-first-name">Pan Number</label>
          <%= f.text_field :pan_number, autofocus: true, placeholder: "Pan Number", class: "form-control" %>
        </div>
      </div>
    </div>
  </div>
  <hr class="my-4">
  <!-- Address -->
  <h6 class="heading-small text-muted mb-4" style="padding: 20px; font-size: 20px;">Contact information</h6>
  <div class="pl-lg-4">
    <div class="row">
      <div class="col-md-4">
        <div class="form-group focused" style="width: auto; padding: 10px;">
          <label class="form-control-label" for="input-address">Address</label>
          <%= f.text_field :address, autofocus: true, placeholder: "Address", class: "form-control" %>
        </div>
      </div>
      <div class="col-lg-4">
        <div class="form-group focused" style="width: auto; padding: 10px;">
          <label class="form-control-label" for="input-city">City</label>
          <%= f.text_field :city, autofocus: true, placeholder: "City", class: "form-control" %>
        </div>
      </div> 
    </div>
    <div class="row">
      <div class="col-lg-4">
        <div class="form-group focused" style="width: auto; padding: 10px;">
          <label class="form-control-label" for="input-city">State</label>
          <%= f.text_field :state, autofocus: true, placeholder: "State", class: "form-control" %>
        </div>
      </div>
      <div class="col-lg-4">
        <div class="form-group focused" style="width: auto; padding: 10px;">
          <label class="form-control-label" for="input-country">Country</label>
          <%= f.text_field :country, autofocus: true, placeholder: "Country", class: "form-control" %>
        </div>
      </div>
      <div class="col-lg-3">
        <div class="form-group focused" style="width: auto; padding: 10px;">
          <label class="form-control-label" for="input-country">Postal code</label>
          <%= f.text_field :zipcode, autofocus: true, placeholder: "Postal Code", class: "form-control" %>
        </div>
      </div>
    </div>
  </div>
  <div style="float: right; padding: 10px;">
    <button class="btn btn-warning">Submit</button>
  </div>

<%end%>

 

  
  <div style="float: left; padding: 10px;">
    <%= link_to "Back", my_profile_dashboard_index_path, class: "btn"%>
  </div>



