


<div class="col-lg-12" style="float: right;">
	
	<div class="row">
		<div class="col-lg-4">
			<div style="width: 335px;">
				<img id="output"/ style="padding: 20px;"><br>
				<input type="file" accept="image/*" onchange="loadFile(event)", class="form-control">
				<center><h3><label>Adhar Card</label></h3></center>
				
			</div>
		</div>
	</div>

	<div class="row">
		<div class="col-lg-4">
			<div style="width: 335px;">
				<img id="output1"/ style="padding: 20px;"><br>
				<input type="file" accept="image/*" onchange="loadFile2(event)", class="form-control">
				<script>
				  var loadFile = function(event) {
				    var output = document.getElementById('output');
				    output.src = URL.createObjectURL(event.target.files[0]);
				    output.onload = function() {
				      URL.revokeObjectURL(output.src) // free memory
				    }
				  };
				  var loadFile2 = function(event) {
				    var output = document.getElementById('output1');
				    output.src = URL.createObjectURL(event.target.files[0]);
				    output.onload = function() {
				      URL.revokeObjectURL(output.src) // free memory
				    }
				  };
				</script>
				<center><h3><label>PAN Card</label></h3></center>
			</div>
		</div>
	</div>

	<div class="row">
		<div class="col-lg-4">
			<%= form_for :kyc_detail, url: update_user_kyc_users_path do |f| %>
			  <h6 class="heading-small text-muted mb-4" style="padding: 20px; font-size: 20px;">Bank Details</h6>
			  <div class="pl-lg-4">
			    <div class="row">
			      <div class="col-lg-4">
			        <div class="form-group focused" style="width: auto; padding: 10px;">
			          <label class="form-control-label" for="input-username">Bank Name</label>
			          <input type="text" id="input-username" class="form-control form-control-alternative" placeholder="Bank Name" value="<%#= @current_user.sponser_id %>" readonly="readonly">
			        </div>
			      </div>
			  	</div>
			    <div class="row">
			       	<div class="col-lg-4">
			        	<div class="form-group focused" style="width: auto; padding: 10px;">
			          		<label class="form-control-label" for="input-username">IFSC Code</label>
		          			<input type="text" id="input-username" class="form-control form-control-alternative" placeholder="IFSC Code" value="<%#= @current_user.sponser_id %>" readonly="readonly">
			          	<%#= f.text_field :invoice_number, placeholder: "Invoice Number", class: "form-control"%>
			        	</div>
			      	</div>
		    	</div>
		    	<div class="row">
			      	<div class="col-lg-4">
			        	<div class="form-group focused" style="width: auto; padding: 10px;">
				          	<label class="form-control-label" for="input-username">Account Holder Name</label>
				          	<input type="text" id="input-username" class="form-control form-control-alternative" placeholder="Account Holder Name" value="<%#= @current_user.email %>" readonly= "readonly">
				        </div>
			      	</div>
			    </div>
			    <div class="row">
			      	<div class="col-lg-4">
			        	<div class="form-group focused" style="width: auto; padding: 10px;">
				          	<label class="form-control-label" for="input-username">Account Number</label>
				          	<input type="text" id="input-username" class="form-control form-control-alternative" placeholder="Account Number" value="<%#= @current_user.email %>" readonly= "readonly">
				        </div>
			      	</div>
			    </div>
			  	<div style="float: right; padding: 10px;">
			    	<button class="btn btn-warning">Update KYC</button>
			 	 </div>
			<%end%>
		</div>
	</div>
</div>
